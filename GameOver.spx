onStart => {
    setXYpos 0, -220
    hide
}

onMsg "game over", => {
    wait 0.5
    gotoFront
    show
    for{
        if ypos >= 0 {
            break
        }
        changeYpos 10.0
        wait 0.02
    }
}

onMsg "menu", =>{
    hide
    setXYpos 0, -220
}
onMsg "reset_timestart", => {
    hide
}