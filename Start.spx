onStart =>{
    broadcast "menu"
    setXYpos 0, -95
}

onStart => {
    for visible {
        if touching(Mouse) {
            setYpos -90
        }else{
            setYpos -95
        }
        wait 0.1
    }
}

onClick => {
    broadcast "start"
    hide
}

onMsg "start", => {
    hide
}

onMsg "menu", => {
	gotoFront
    show
}
